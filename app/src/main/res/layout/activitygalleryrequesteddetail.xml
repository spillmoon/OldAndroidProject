<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.changeui.userinterface.userinterface.DeleteReceiveRequest"
    android:background="#FFFFFF">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:background="#FFFFFF">
        <ImageView
            android:id="@+id/myRequestedimage"
            android:layout_width="300dp"
            android:layout_height="300dp" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="사진이름 : "
                android:textSize="20dp"
                android:background="#FFFFFF"/>
            <TextView
                android:id="@+id/requestIdDetail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:background="#FFFFFF"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <Button
                android:id="@+id/requestAcceptCancle"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="요청취소"
                android:background="@drawable/listbtn"/>
            <Button
                android:id="@+id/canclecancle"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:text="뒤로가기"
                android:background="@drawable/listbtn"
                android:layout_marginLeft="5dp"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
